@{
    ViewBag.Title = "Contato";
}


<div id="myCarousel" class="carouselPages slide">
    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <div class="contact-img"></div>
            <div class="carousel-caption-pages">
                <h1>Contato</h1>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Contato <small>Nós queremos ouvir você</small></h1>
            <ol class="breadcrumb">
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li class="active">Contato</li>
            </ol>
        </div>
    </div><!-- /.row -->
    <div class="row">
        <div class="col-sm-8">
            <h1>Email enviado com sucesso. Retornaremos em breve!</h1>
        </div>
    </div><!-- /.row -->
</div>